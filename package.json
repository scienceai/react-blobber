{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "react boilerplate project starter kit",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=\"production\" node babel-server.js",
    "dev": "node_modules/.bin/concurrent --kill-others \"npm run start-dev\" \"npm run watch-client\"",
    "start-dev": "NODE_ENV=\"development\" node babel-server.js",
    "watch-client": "node frontend-dev-server.js",
    "build": "node node_modules/.bin/webpack --verbose --colors --display-error-details --config -d webpack.production.config.js",
    "test": "node_modules/.bin/mocha --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-analytics/react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "hot-loader"
  ],
  "author": "Chris Callahan <chris@standardanalytics.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/standard-analytics/react-boilerplate/issues"
  },
  "homepage": "https://github.com/standard-analytics/react-boilerplate",
  "dependencies": {
    "body-parser": "1.13.2",
    "classnames": "^2.1.3",
    "cookie-parser": "^1.3.5",
    "cookie-session": "^1.2.0",
    "csurf": "^1.8.3",
    "es6-promise": "^2.3.0",
    "express": "^4.13.0",
    "history": "^1.9.1",
    "isomorphic-fetch": "^2.1.0",
    "json-loader": "^0.5.2",
    "moment": "^2.10.5",
    "react": "0.14.0-rc1",
    "react-dom": "0.14.0-rc1",
    "react-router": "1.0.0-rc1",
    "react-tap-event-plugin": "^0.1.7",
    "serve-static": "^1.10.0",
    "uuid": "^2.0.1",
    "whatwg-fetch": "0.9.0"
  },
  "devDependencies": {
    "babel": "^5.6.3",
    "babel-core": "^5.6.3",
    "babel-eslint": "^3.1.18",
    "babel-loader": "^5.1.4",
    "concurrently": "0.1.1",
    "css-loader": "^0.15.1",
    "cssnext-loader": "^1.0.1",
    "eslint": "^0.23.0",
    "eslint-plugin-react": "^2.5.2",
    "file-loader": "^0.8.4",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "piping": "^0.2.0",
    "react-hot-loader": "^1.2.7",
    "request": "^2.60.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
